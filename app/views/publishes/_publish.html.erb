<div id="<%= dom_id publish %>">
  
  <p>
    <strong>Creado:</strong>
    <%= publish.created_at.to_date %>
  </p>

  <p>
    <strong>Titulo:</strong>
    <%= publish.title %>
  </p>

  <p>
    <strong>Contenido:</strong>
    <%= publish.content %>
  </p>

  <p>
    <strong>Me Gusta:</strong>
    <%= publish.likes %>
  </p>

  <p>
    <strong>No me Gusta:</strong>
    <%= publish.dislikes %>
  </p>

</div>
